<%= render 'shared/topbar' %>
<p> Main page </p>

<h2> Welcome to Blogs Api </h2>

<%= render 'shared/flash_messages' %>

<% if @body.empty? == false %>
  <% @body["data"].each do |blog| %>
    <table>
      <tr>
        <th> Title </th>
        <th> Body </th>
      </tr>
      <tr>
        <td> <%= blog["attributes"]["title"] %></td>
        <td> <%= blog["attributes"]["body"] %> </td>
      </tr>
    </table>
  <% end %>
<% end %>

